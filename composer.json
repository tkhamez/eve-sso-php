{
    "name": "tkhamez/eve-sso",
    "description": "EVE Online - SSO",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-gmp": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.7",
        "league/oauth2-client": "^2.7",
        "web-token/jwt-core": "^2.2",
        "web-token/jwt-signature": "^2.2",
        "web-token/jwt-signature-algorithm-rsa": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "web-token/jwt-key-mgmt": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Eve\\Sso\\": "src",
            "Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html xdebug/coverage"
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        },
        "sort-packages": true
    }
}
