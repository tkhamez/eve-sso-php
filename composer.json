{
    "name": "tkhamez/eve-sso",
    "description": "EVE Online - SSO",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-gmp": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "league/oauth2-client": "^2.2",
        "web-token/jwt-core": "^2.2",
        "web-token/jwt-signature": "^2.0",
        "web-token/jwt-signature-algorithm-rsa": "^2.0"
    },
    "require-dev": {
        "web-token/jwt-key-mgmt": "^2.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Eve\\Sso\\": "src",
            "Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "platform": {
            "php": "7.3.0"
        },
        "sort-packages": true
    }
}
